<?xml version="1.0" encoding="utf-8"?>
<com.ramotion.foldingcell.FoldingCell xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/folding_cell_meal"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    app:backSideColor="@color/colorAccent">




    <include
        android:id="@+id/meal_plan_details_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        layout="@layout/fragment_meal_plan_details"/>

    <include
        android:id="@+id/meal_plan_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/fragment_meal_plan"/>

</com.ramotion.foldingcell.FoldingCell>